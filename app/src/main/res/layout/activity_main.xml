<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.jm.android.jmtoken.Main">

    <LinearLayout android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:orientation="vertical" tools:layout_editor_absoluteY="0dp"
                  tools:layout_editor_absoluteX="0dp">
        <EditText
                android:id="@+id/etUid"
                android:imeOptions="actionDone"
                android:singleLine="true"
                android:inputType="text"
                android:hint="请输入uid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        <Button android:layout_width="312dp" android:layout_height="89dp" android:text="下载视频"
                android:onClick="parseSign" tools:layout_editor_absoluteY="549dp" tools:layout_editor_absoluteX="49dp"/>
        <EditText
                android:id="@+id/etName"
                android:imeOptions="actionDone"
                android:singleLine="true"
                android:inputType="text"
                android:hint="请输入用户名"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        <Button android:layout_width="312dp" android:layout_height="89dp" android:text="获取用户列表"
                android:onClick="searchUserList" tools:layout_editor_absoluteY="549dp" tools:layout_editor_absoluteX="49dp"/>
    <LinearLayout
            android:id="@+id/ll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
             >
    </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>